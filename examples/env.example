REDPANDA_CONSOLE_PORT=18082
WORKFLOW_PORT=18080
CRM_PORT=18081
SIGN_PORT=18084
LEDGER_PORT=18083

# BankID test adapter
BANKID_MODE=mock
BANKID_BASE_URL=https://appapi2.test.bankid.com/rp/v6.0
